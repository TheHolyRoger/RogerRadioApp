{
  "appId": "com.theholyroger.rogerradio",
  "appName": "RogerRadio",
  "bundledWebRuntime": false,
  "npmClient": "yarn",
  "webDir": "build",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    }
  },
  "ios": {
    "contentInset": "always"
  },
  "server": {
    "hostname": "rogerradio.app.local",
    "androidScheme": "https"
  },
  "cordova": {
    "preferences": {
      "Hostname": "rogerradio.app.local"
    }
  }
}
